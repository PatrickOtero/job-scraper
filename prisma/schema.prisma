generator client {
  provider = "prisma-client-js"
  output   = "./generated/prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

model Job {
  id       Int     @id @default(autoincrement())
  title    String
  company  String?
  location String?
  time     String?
  link     String
  job_name String  @unique
}
